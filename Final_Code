    {
      "cell_type": "code",
      "source": [
        "#Left-Handed Pitchers\n",
        "left = left.dropna()\n",
        "l_X = left.drop(['CO_home', 'HR'], axis = 1)\n",
        "l_y = left[['CO_home']]\n",
        "l_model = LogisticRegression(random_state = 0, max_iter = 10).fit(l_X, l_y)\n",
        "l_prob = l_model.predict_proba(l_X)[:, 1]\n",
        "sns.histplot(l_prob)"
      ],
